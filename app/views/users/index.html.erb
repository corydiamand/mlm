<%= will_paginate %>

<%= form_for(current_user) do |f| %>
  <div class="field">
    <%=f.label :search_name %>
    <%=f.text_field :search_name, data: { autocomplete_source: search_users_path } %>
  </div>
<% end %>

<ul class="users">
	<% @users.each do |user| %>
		<li>
			<%= link_to "#{user.last_name}, #{user.first_name}", user %>
		</li>
	<% end %>
</ul>

<%= will_paginate %>